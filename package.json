{
  "name": "starter.react",
  "version": "1.4.0",
  "description": "Starter kit for React",
  "author": "Abraham Tewa",
  "license": "MIT",
  "homepage": "https://github.com/AbrahamTewa/starter.react",
  "engines": {
    "node": ">=8.9.0"
  },
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production gulp prepare:build && webpack -p",
    "clean": "gulp clean",
    "doc": "run-p doc:*",
    "doc:jsdoc": "gulp clean:doc:jsdoc && jsdoc -c ./jsdoc.config.json --readme README.md",
    "doc:storybook": "gulp clean:doc:storybook && build-storybook -c storybook -o doc/storybook",
    "lint": "run-p lint:*",
    "lint:javascript": "eslint .",
    "lint:markdown": "remark . --quiet --frail",
    "lint:style": "stylelint \"./**/*.?(c|sc|sa)ss\"",
    "semantic-release": "semantic-release",
    "audit": "npm audit --json | npm-audit-html --output reports/audit.html",
    "start": "node server.js",
    "test": "jest --config jest.config.json",
    "test-cov": "gulp clean:report:coverage && jest --config jest.config.json --coverage",
    "watch": "webpack-dev-server --progress --hot -d",
    "watch:storybook": "start-storybook -p 9001 -c storybook"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AbrahamTewa/starter.react"
  },
  "dependencies": {
    "history": "^4.10.1",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.4",
    "remove": "^0.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/register": "^7.6.2",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/git": "^7.0.16",
    "@storybook/addon-actions": "^5.2.4",
    "@storybook/addon-storyshots": "^5.2.4",
    "@storybook/react": "^5.2.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "del": "^5.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-import": "^0.13.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.19.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "express": "^4.17.1",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean-css": "^4.2.0",
    "gulp-cli": "^2.2.0",
    "gulp-copy": "^4.0.1",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-junit": "^8.0.0",
    "jsdoc": "^3.6.3",
    "jsdoc-oblivion": "^0.0.9",
    "minami": "^1.1.1",
    "node-sass": "^4.12.0",
    "npm-audit-html": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "react-test-renderer": "^16.10.2",
    "remark-cli": "^7.0.0",
    "remark-preset-lint-recommended": "^3.0.3",
    "sass-loader": "^8.0.0",
    "semantic-release": "^15.13.27",
    "source-map-support": "^0.5.13",
    "style-loader": "^1.0.0",
    "stylelint": "^11.1.1",
    "stylelint-webpack-plugin": "^1.0.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  },
  "optionalDependencies": {
    "fsevents": "*"
  }
}
