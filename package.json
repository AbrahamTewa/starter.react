{
  "name": "starter.react",
  "version": "1.2.0",
  "description": "Starter kit for React",
  "author": "Abraham Tewa",
  "license": "MIT",
  "homepage": "https://github.com/AbrahamTewa/starter.react",
  "engines": {
    "node": ">=8.9.0"
  },
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production gulp prepare:build && webpack -p",
    "clean": "gulp clean",
    "doc": "run-p doc:*",
    "doc:jsdoc": "gulp clean:doc:jsdoc && jsdoc -c ./jsdoc.config.json --readme README.md",
    "doc:storybook": "gulp clean:doc:storybook && build-storybook -c storybook -o doc/storybook",
    "lint": "run-p lint:*",
    "lint:javascript": "eslint .",
    "lint:markdown": "remark . --quiet --frail",
    "lint:style": "stylelint \"./**/*.?(c|sc|sa)ss\"",
    "semantic-release": "semantic-release",
    "start": "node server.js",
    "test": "jest --config jest.config.json",
    "test-cov": "gulp clean:report:coverage && jest --config jest.config.json --coverage",
    "watch": "webpack-dev-server --progress --hot -d",
    "watch:storybook": "start-storybook -p 9001 -c storybook"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AbrahamTewa/starter.react"
  },
  "dependencies": {
    "history": "^4.7.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-redux": "^6.0.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.1",
    "remove": "^0.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/git": "^7.0.8",
    "@storybook/addon-actions": "^4.1.11",
    "@storybook/addon-storyshots": "^4.1.11",
    "@storybook/react": "^4.1.11",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.1.1",
    "css-loader": "^2.1.0",
    "del": "^3.0.0",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint": "^5.14.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-import": "^0.13.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "express": "^4.16.2",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-clean-css": "^4.0.0",
    "gulp-cli": "^2.0.1",
    "gulp-copy": "^4.0.1",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.4",
    "html-webpack-plugin": "^3.2.0",
    "istanbul": "^0.4.5",
    "jest": "^24.1.0",
    "jest-junit": "^6.3.0",
    "jsdoc": "^3.5.5",
    "jsdoc-oblivion": "^0.0.9",
    "minami": "^1.1.1",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "react-test-renderer": "^16.8.2",
    "remark-cli": "^6.0.1",
    "remark-preset-lint-recommended": "^3.0.2",
    "sass-loader": "^7.1.0",
    "semantic-release": "^15.13.3",
    "source-map-support": "^0.5.10",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-webpack-plugin": "^0.10.5",
    "webpack": "^4.29.4",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  },
  "optionalDependencies": {
    "fsevents": "*"
  }
}
