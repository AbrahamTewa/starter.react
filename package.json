{
  "name": "starter.nodejs",
  "version": "0.2.0",
  "description": "Starter kit for ReactJS",
  "author": "Abraham Tewa",
  "license": "MIT",
  "homepage": "https://github.com/AbrahamTewa/starter.reactjs",
  "engines": {
    "node": ">=8.9.0"
  },
  "private": true,
  "scripts": {
    "analysis": "gulp clean:report:analysis && es6-plato --dir reports/analysis --recurse --title \"Starter React\" src/**/*.js",
    "build": "cross-env NODE_ENV=production gulp prepare:build && webpack -p",
    "clean": "gulp clean",
    "doc": "run-p doc:*",
    "doc:jsdoc": "gulp clean:doc:jsdoc && jsdoc -c ./jsdoc.config.json --readme README.md",
    "doc:storybook": "gulp clean:doc:storybook && build-storybook -c storybook -o doc/storybook",
    "lint": "run-p lint:*",
    "lint:markdown": "remark . --quiet --frail",
    "lint:javascript": "eslint .",
    "lint:style": "stylelint .",
    "start": "node server.js",
    "test": "jest --config jest.config.json",
    "test-cov": "gulp clean:report:coverage && jest --config jest.config.json --coverage",
    "watch": "webpack-dev-server --progress --hot -d",
    "watch:storybook": "start-storybook -p 9001 -c storybook"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AbrahamTewa/starter.reactjs"
  },
  "dependencies": {
    "history": "^4.7.2",
    "prop-types": "^15.6.0",
    "react": "^16.1.0",
    "react-dom": "^16.2.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.1",
    "remove": "^0.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@storybook/addon-actions": "^4.1.4",
    "@storybook/addon-storyshots": "^4.1.4",
    "@storybook/react": "^4.1.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-istanbul": "^5.1.0",
    "cross-env": "^5.1.1",
    "css-loader": "^2.1.0",
    "del": "^3.0.0",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "es6-plato": "^1.0.18",
    "eslint": "^5.11.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-react": "^7.12.0",
    "express": "^4.16.2",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-clean-css": "^4.0.0",
    "gulp-cli": "^2.0.1",
    "gulp-copy": "^4.0.1",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.4",
    "html-webpack-plugin": "^3.2.0",
    "istanbul": "^0.4.5",
    "jest": "^23.6.0",
    "jsdoc": "^3.5.5",
    "jsdoc-oblivion": "^0.0.9",
    "minami": "^1.1.1",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "react-test-renderer": "^16.1.0",
    "remark-cli": "^6.0.1",
    "remark-preset-lint-recommended": "^3.0.2",
    "sass-loader": "^7.1.0",
    "source-map-support": "^0.5.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.9.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  },
  "optionalDependencies": {
    "fsevents": "*"
  }
}
